commands:
    composer_update:
        command: export COMPOSER_HOME=/root && /usr/bin/composer.phar self-update

container_commands:
    02-clearConfigCache:
        command:
            "php artisan config:clear"
    03-clearConfigCache:
        command:
            "php artisan cache:clear"
    04-clearConfigCache:
        command:
            "php artisan view:clear"
    05-migrations:
        command: "php artisan migrate"
        cwd: "/var/app/ondeck"